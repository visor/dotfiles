URxvt.italicFont:     xft:Consolas:style=italic:pixelsize=13,xft:unifont
URxvt.boldItalicFont: xft:Consolas:style=bold italic:pixelsize=13,xft:unifont

URxvt.foreground: #FFFFFF
URxvt.background: #000000

