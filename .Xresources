Xft.antialias: 1
Xft.dpi:       96
Xft.hinting:   true
Xft.hintstyle: hintmedium
Xft.rgba:      rgb

urxvt*saveLines:      10240
URxvt.transparent:    false
URxvt.scrollBar:      false
URxvt.font:           xft:Consolas:pixelsize=13,xft:unifont
